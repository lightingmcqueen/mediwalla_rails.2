<%- model_class = Clinic -%>
<div class="page-header">
  <h1><%=t '.title', :default => Clinic %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:title) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.title %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address_line_1) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.address_line_1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address_line_2) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.address_line_2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:country_id) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.country_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone_country_code) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.phone_country_code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zipcode) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.zipcode %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ratings) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.ratings %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:coordinator) %></dt>
      <dd><pre class="prettyprint"><%= @clinic.coordinator %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              clinics_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_clinic_path(@clinic), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              clinic_path(@clinic),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

