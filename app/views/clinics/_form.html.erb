<%= form_for @clinic, :html => { :class => 'form-horizontal form-model' } do |f| %>
  <div class="form-group">
    <%= f.label :title, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :title, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :description, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :description, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :address_line_1, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_area :address_line_1, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :address_line_2, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_area :address_line_2, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :city, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :city, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :country_id, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :country_id, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :phone_country_code, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :phone_country_code, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :zipcode, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :zipcode, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :ratings, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :ratings, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :coordinator, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :coordinator, :class => 'text-field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  clinics_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
